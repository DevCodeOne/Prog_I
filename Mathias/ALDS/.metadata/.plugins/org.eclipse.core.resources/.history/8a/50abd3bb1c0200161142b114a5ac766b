package Uebung130;

public class Stack {
	Object[] s;
	int index = -1;
	public Stack(){
		s = new Object[20];
	}
	
	public Stack Push(Object o){
		if (index>=20) {
			throw new StackOverflow();
		} else {
			return this;
		}
		index++;
		return this;
		
	}
	
	public Stack Pop(){
		if (index<0) {
			throw new StackUnderflow();
		} else {
			return this;
		}
		index--;
		return this;
		
		
	}
	
	public Object Top(){
		if (index<0) {
			throw new StackUnderflow();
		} else {
			return s[index];
		}
		
		return s[index--];
	}
	
	public boolean empty(){		
		return index<0?true:false;
	}
}
