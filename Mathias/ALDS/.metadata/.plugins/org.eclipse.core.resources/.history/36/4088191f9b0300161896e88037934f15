package Uebung130;

import java.util.Scanner;

public class TestStack {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Stack s = new Stack();
		first(s);
	}

	public static void first(Stack s) {
		s.push(read());
		write(s.top());
		s.pop();
		s.push(read());
	}

	public static void second(Stack s) {
		write(s.top());
		s.pop();
		write(s.top());
		s.pop();
		s.push(read());
	}

	public static void third(Stack s) {
		s.pop();
		s.push(s.top());
		write(s.top());
		s.pop();
		s.push(read());
	}

	public void write(Object o){
		System.out.println(o);		
	}
	
	public Object read(){
		Scanner sc = new Scanner(System.in);
		return sc.next();
	}
}
}
